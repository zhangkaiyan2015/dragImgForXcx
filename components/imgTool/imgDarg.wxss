/* components/imgTool/imgDarg.wxss */

.img-view {
    position: relative;
    height: 632rpx;
    background-color: #ffffff;
}

.img-view .img-box {
    position: absolute;
    width: 33.3%;
    padding: 10rpx;
    overflow: hidden;
    box-sizing: border-box;
    height: 212rpx;
}

.img-view .img-box.img-box1 {
    width: 66.6%;
    height: 420rpx;
}

.img-box .img {
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
}