/* components/imgTool/imgDarg.wxss */

.img-view {
    position: relative;
    height: 848rpx;
    background-color: #ffffff;
}

.img-view .img-box {
    position: absolute;
    padding: 10rpx;
    overflow: hidden;
    box-sizing: border-box;
}

.img-box .img {
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
}